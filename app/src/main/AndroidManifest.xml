            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Chat.MessageActivity" />

        <service android:name=".MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
    </application>

</manifest>